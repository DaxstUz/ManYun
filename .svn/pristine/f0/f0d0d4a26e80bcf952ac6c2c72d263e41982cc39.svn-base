package com.ch.mhy.entity;

import java.io.Serializable;

import android.widget.ImageView;
import android.widget.ProgressBar;
import android.widget.TextView;

import com.ch.mhy.download.ImageDownProgress;
/**
 * 漫画下载话信息
 * @author wenli.liu
 *
 */
public class Down implements Serializable{

	private static final long serialVersionUID = 1L;
	
	private ComicsDetail cd;//漫画基本信息
    
	private Integer isdonw=1; //下载状态  0表示完成，1表示正在下载,2表示已暂停下载,3表示等待状态
    
	private Integer downs=0;//下载了几话
	
	private boolean downflag = true;//是否可以下载，暂停的时候设为 false
    
	private ImageDownProgress imageDownProgress;//下载器
	
	private TextView tx_down_percentage;//下载百分比及状态显示文字
	
	private ProgressBar progressBar;//进度条
	
	private ImageView down_status_image;//下载状态图标
	

	public Integer getDowns() {
		return downs;
	}

	public void setDowns(Integer downs) {
		this.downs = downs;
	}

	public ComicsDetail getCd() {
		return cd;
	}

	public void setCd(ComicsDetail cd) {
		this.cd = cd;
	}

	public Integer getIsdonw() {
		return isdonw;
	}

	public void setIsdonw(Integer isdonw) {
		this.isdonw = isdonw;
	}

	public ImageDownProgress getImageDownProgress() {
		return imageDownProgress;
	}

	public void setImageDownProgress(ImageDownProgress imageDownProgress) {
		this.imageDownProgress = imageDownProgress;
	}

	public TextView getTx_down_percentage() {
		return tx_down_percentage;
	}

	public void setTx_down_percentage(TextView tx_down_percentage) {
		this.tx_down_percentage = tx_down_percentage;
	}

	public ProgressBar getProgressBar() {
		return progressBar;
	}

	public void setProgressBar(ProgressBar progressBar) {
		this.progressBar = progressBar;
	}

	public ImageView getDown_status_image() {
		return down_status_image;
	}

	public void setDown_status_image(ImageView down_status_image) {
		this.down_status_image = down_status_image;
	}


	public boolean isDownflag() {
		return downflag;
	}

	public void setDownflag(boolean downflag) {
		this.downflag = downflag;
	}

}
